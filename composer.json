{
    "name": "davidsneighbour/dropbox-php-sdk",
    "type": "library",
    "description": "Dropbox PHP API V2 SDK (Unofficial, for PHP 7.2+)",
    "keywords" : ["dropbox", "sdk", "api", "client", "php", "php7.2", "unofficial"],
    "homepage": "https://github.com/davidsneighbour/dropbox-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunal Varma",
            "email": "kunalvarma05@gmail.com"
        },
        {
            "name": "Patrick Kollitsch",
            "email": "patrick@davids-neighbour.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "tightenco/collect": "^5.6.33"
    },
    "autoload": {
        "psr-4": {
            "Dropbox\\": "src/Dropbox"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dropbox\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.27",
        "mikey179/vfsstream": "^1.6.11"
    }
}
